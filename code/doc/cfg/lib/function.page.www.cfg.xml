<?xml version="1.0" encoding="ISO-8859-1"?>
<config>
  <tree>
     <delim>/</delim>
     <path>library/function</path>
  </tree>


  <xmldoc>
    <tagdb type="file">tagdb/function.tagdb.xml</tagdb>
    <root type="env">URLBASE</root>
    <xml>
      <element>
      <name>name</name>  
      <default type="text">None</default>
      </element>

      <element>
      <name>syntax</name>  
      <default type="text">None</default>
      </element>

      <element>
      <name>header</name>  
      <default type="text">None</default>
      </element>


      <element>
      <name>description</name>  
      <default type="text">None</default>
      </element>

      <element>
      <name>returns</name>  
      <default type="text">None</default>
      </element>

      <element>
      <name>errors</name>  
      <default type="text">None</default>
      </element>

      <element>
      <name>example</name>  
      <default type="text">None</default>
      <modifiers>
         <attr>
         <name>source</name>
         <type>rawfile</type>
         <path>tmp</path>
         <action>&lt;br&gt;<center>Source Code: <a href="src/&filename;">&filename;</a></center>&lt;br&gt;<table width="540" cellpadding="4"><tr><td class="docbox"><pre width="60">&file;</pre></td></tr></table>&lt;br&gt;</action>
         </attr>

         <attr>
         <name>image</name>
         <type>text</type>
         <action>&lt;br&gt;<center><img src="img/&xml;.gif"></img>&lt;br&gt;</center>
         </action>
         </attr>

         <attr>
         <name>postscript</name>
         <type>text</type>
         <action>&lt;br&gt;<center><a href="img/&xml;.ps.gz">Download PostScript</a></center>
         </action>
         </attr>

         <attr>
         <name>rplot</name>
         <type>text</type>
         <action>&lt;br&gt;<center><img src="img/&xml;.gif"></img>&lt;br&gt;<a href="img/&xml;.ps.gz">Download PostScript</a>|<a href="img/&xml;.rp.xml.gz">Download rXML</a></center>
         </action>
         </attr>


         <attr>
         <name>url</name>
         <type>text</type>
         <action>&lt;br&gt;<center><a href="src/&xml;">&xml;</a>&lt;br&gt;</center>
         </action>
         </attr>



      </modifiers>
      </element>


    </xml>
  </xmldoc>

  <map>
     <entry>
        <search>NAME</search>
        <replace>name</replace>
     </entry>

     <entry>
        <search>SYNTAX</search>
        <replace>syntax</replace>
     </entry>

     <entry>
        <search>HEADER</search>
        <replace>header</replace>
     </entry>


     <entry>
        <search>DESCRIPTION</search>
        <replace>description</replace>
     </entry>

     <entry>
        <search>RETURNS</search>
        <replace>returns</replace>
     </entry>

     <entry>
        <search>ERRORS</search>
        <replace>errors</replace>
     </entry>


     <entry>
        <search>EXAMPLE</search>
        <replace>example</replace>
     </entry>

  </map>

  <external type="file">tmp/libname.xml</external>
   
  <script type="file">sc/lib/function.page.www.sc</script>

  <match>
     <element>name</element>
  </match>

</config>


