<?xml version="1.0" encoding="ISO-8859-1"?>
<config>
  <tree>
     <delim>/</delim>
     <path>library/structure</path>
  </tree>

  <xmldoc>
    <tagdb type="file">tagdb/struct.tagdb.xml</tagdb>
    <root type="env">URLBASE</root>
    <xml>
      <element>
      <name>name</name>  
      <default type="text">None</default>
      </element>

      <element>
      <name>struct</name>  
      <default type="text">None</default>
      </element>

      <element>
      <name>header</name>  
      <default type="text">None</default>
      </element>


      <element>
      <name>description</name>  
      <default type="text">None</default>
      </element>
    </xml>
  </xmldoc>

  <map>
     <entry>
        <search>NAME</search>
        <replace>name</replace>
     </entry>

     <entry>
        <search>STRUCT</search>
        <replace>struct</replace>
     </entry>

     <entry>
        <search>HEADER</search>
        <replace>header</replace>
     </entry>


     <entry>
        <search>DESCRIPTION</search>
        <replace>description</replace>
     </entry>
  </map>

  <external type="file">tmp/libname.xml</external>
  
  <script type="file">sc/lib/struct.page.local.sc</script>

  <match>
     <element>name</element>
  </match>

</config>




